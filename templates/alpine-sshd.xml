<?xml version="1.0"?>
<Container>
  <Name>sshd_server</Name>
  <Beta>False</Beta>
  <Repository>patricol/alpine-sshd:latest</Repository>
  <Registry>https://hub.docker.com/r/patricol/alpine-sshd/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>False</Privileged>
  <Support>https://github.com/Patricol/docker-templates/issues</Support>
  <Project>https://hub.docker.com/r/patricol/alpine-sshd/</Project>
  <Overview>Alpine OpenSSH server with easy user creation.</Overview>
  <Category></Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/patricol/docker-templates/master/templates/alpine-sshd.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/patricol/docker-templates/master/images/alpine-sshd.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <DonateImg/>
  <MinVer/>
  <Description>Alpine OpenSSH server with easy user creation.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>22</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/sshd/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/data_to_mount/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
</Container>
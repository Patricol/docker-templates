<?xml version="1.0"?>
<Container version="2">
  <Name>wallabag</Name>
  <Repository>wallabag/wallabag</Repository>
  <Registry>https://hub.docker.com/r/wallabag/wallabag/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>https://github.com/Patricol/docker-templates/issues</Support>
  <Project>https://github.com/wallabag/wallabag/</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/patricol/docker-templates/master/templates/wallabag.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/patricol/docker-templates/master/images/wallabag.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort/>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment/>
  <Labels/>
  <Config Name="Web UI Port" Target="80" Default="" Mode="tcp" Description="Port used for viewing Wallabag." Type="Port" Display="always" Required="false" Mask="false"/>
</Container>
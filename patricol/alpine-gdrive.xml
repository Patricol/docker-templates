    <Variable>
      <Name>CLIENT_ID</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>CLIENT_SECRET</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>VERIFICATION_CODE</Name>
      <Value>See description</Value>
    </Variable>
    <Variable>
      <Name>MOUNT_OPTS</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
</Container>
<?xml version="1.0"?>
<Container>
  <Name>google_music_manager</Name>
  <Beta>False</Beta>
  <Repository>jaymoulin/google-musicmanager:latest</Repository>
  <Registry>https://hub.docker.com/r/jaymoulin/google-musicmanager/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>False</Privileged>
  <Support>https://github.com/Patricol/docker-templates/issues</Support>
  <Project>https://hub.docker.com/r/jaymoulin/google-musicmanager/</Project>
  <Overview>This image allows you to download and upload your Google Music Library to/from a selected folder. SSH into the container and run "auth" to setup, then restart the container.</Overview>
  <Category></Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/patricol/docker-templates/master/patricol/google-musicmanager.xml</TemplateURL>
  <Icon>https://gitlab.com/jaymoulin/docker-google-musicmanager/raw/master/logo.png</Icon>
  <ExtraParams>--restart=always</ExtraParams>
  <PostArgs/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <DonateImg/>
  <MinVer/>
  <Description>This image allows you to download and upload your Google Music Library to/from a selected folder. SSH into the container and run "auth" to setup, then restart the container.</Description>
  <Networking/>
  <Data>
    <Volume>
      <HostDir>/mnt/user/google_music_manager/to_upload/</HostDir>
      <ContainerDir>/media/library/upload</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/google_music_manager/downloaded_library/</HostDir>
      <ContainerDir>/media/library/download</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/google_music_manager_oauth/</HostDir>
      <ContainerDir>/root/oauth</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Name>REMOVE</Name>
      <Value>true</Value>
    </Variable>
    <Variable>
      <Name>ONESHOT</Name>
      <Value>false</Value>
    </Variable>
    <Variable>
      <Name>UPLOADER_ID</Name>
      <Value>false</Value>
    </Variable>
  </Environment>
</Container>